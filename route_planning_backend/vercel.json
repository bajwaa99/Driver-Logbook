{
  "builds": [
    {
      "src": "route_planning_backend/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "route_planning_backend/wsgi.py"
    }
  ],
  "functions": {
    "api/trip/calculate": {
      "runtime": "python3.9",
      "includeFiles": ["route_planning_backend/api/migrations/**"]
    }
  }
}