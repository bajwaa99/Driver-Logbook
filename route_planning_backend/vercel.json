{
  "builds": [
    {
      "src": "route_planning_backend/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    },
    {
      "src": "route_planning_backend/api/**/*.py",
      "use": "@vercel/python"
  }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "route_planning_backend/wsgi.py"
    }
  ]
}