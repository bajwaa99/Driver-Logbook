{
    "builds": [{
        "src": "route_planning_backend/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "route_planning_backend/wsgi.py" 
        }
    ]
}